Location log
<table width="70%">
  <col width = 20%>
  <col width = 30%>
  <col width = 25%>
  <col width = 25%>
  <tr>
    <td>User</td>
    <td>Time</td>
    <td>Longitude</td>
    <td>Latitude</td>
  </tr>
   <% Positionlog.all.each do |log| %>
     <tr>
       <td><%= log.user_name %></td>
       <td><%= log.created_at.to_s %></td>
       <td><%= log.longitude.to_s %></td>
       <td><%= log.latitude.to_s %></td>
     </tr>
  <% end %>
</table>
