<!DOCTYPE html>
<html>
  <head>
    <title>Contacts</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body style="background-color: #d3d3e0; font-family: Arial;" >

    <table style="width:100%" align="center" >
      <col width="15%">
      <col width="60%">
      <col width="15%">

      <tr>

        <td align="center" style="font-size: 40px"; >
          <%= link_to "Home", root_path %>
        </td>


        <td align="center" style="font-size: 50pt; font-weight: bold">
          Contacts sheets
        </td>

        <td align="center" style="font-weight: bold">
          <% if false %>
            <%= link_to "Reset cookie consent", reset_cookie_consent_path %><br/>
            <%= link_to "Test", test_path %><br/>
          <% end %>
          <% if !cookies.signed[:logged_in].blank? %>
            <%= link_to "Log out", log_out_path %>
          <% end %>

        </td>

      </tr>
    </table>


  <%= yield %>

  </body>
</html>
