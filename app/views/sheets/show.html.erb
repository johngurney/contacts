<p id="notice"><%= notice %></p>

<p>
  <strong>Client name:</strong>
  <%= @sheet.client_name %><br/>
  <strong>Internal ID :</strong>
  <%= @sheet.number %><br/>
  <strong>ID number :</strong>
  <%= @sheet.id.to_s %>
</p>

<% if !@sheet.number.blank? %>
  <%= link_to "See sheet",  contact_sheet_path(@sheet.number) %><br/><br/>
<% end %>

<% @sheet.contacts.each do|contact| %>
  <%= contact.name %><br/>
<% end %>

<%= link_to 'Edit', edit_sheet_path(@sheet) %> |
<%= link_to 'Back', sheets_path %>
